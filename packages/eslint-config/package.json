{
  "name": "@vp-tw/eslint-config",
  "version": "0.0.14",
  "description": "ViPro's ESLint configuration",
  "homepage": "https://github.com/vdustr/eslint-config",
  "author": {
    "name": "ViPro",
    "url": "https://vdustr.dev"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.mjs"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./README.md": "./README.md",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:unbuild fixEslintTypegenDts",
    "build:unbuild": "unbuild",
    "copy": "tsx scripts/copy",
    "fixEslintTypegenDts": "tsx scripts/fixEslintTypegenDts",
    "prepare": "run-s typegen",
    "prepublishOnly": "run-s build copy",
    "typegen": "tsx scripts/typegen"
  },
  "dependencies": {
    "@mui/types": "catalog:",
    "@types/eslint-config-prettier": "catalog:",
    "es-toolkit": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-flat-config-utils": "catalog:",
    "local-pkg": "catalog:"
  },
  "devDependencies": {
    "eslint-typegen": "^1.0.0",
    "unbuild": "^3.3.1"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-mdx": "catalog:",
    "eslint-plugin-package-json": "catalog:",
    "eslint-plugin-react-compiler": "catalog:",
    "eslint-plugin-storybook": "catalog:"
  },
  "peerDependenciesMeta": {
    "eslint-mdx": {
      "optional": true
    },
    "eslint-plugin-package-json": {
      "optional": true
    },
    "eslint-plugin-react-compiler": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
